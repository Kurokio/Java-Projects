  public class Term {
    public int coef;
    public int exp;
    
    // constructor
    public Term(int coeff, int expo) {
    coef = coeff;
    exp = expo;
    }
    
    // methods
    public int getCoef() {
      return coef;
    }
    
    public int getExp() {
      return exp;
    }
  } // end of class
  